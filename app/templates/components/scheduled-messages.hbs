<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Schedule message id</th>
      <th>Message Id</th>
      <th>Type</th>
      <th>Timestamp</th>
      <th>Due at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    {{#each messages as |msg|}}
      <tr>
        <td>{{msg.scheduledMessageId}}</td>
        <td>{{msg.message.messageId}}</td>
        <td>{{msg.message.messageType}}</td>
        <td>{{moment-format msg.message.timestamp "lll"}}</td>
        <td>{{moment-format msg.dueTime "lll"}}</td>
        <td>
          <a href="#" {{action onDelete msg}} title="Delete">{{fa-icon 'trash'}}</a>
          <a href="#" {{action onEnqueue msg}} title="Enqueue">{{fa-icon 'refresh'}}</a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
<div>
  Total {{meta.total}} messages
</div>
