<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Message Id</th>
      <th>Type</th>
      <th>Timestamp</th>
      <th>Killed at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    {{#each messages as |msg|}}
      <tr>
        <td>{{msg.messageId}}</td>
        <td>{{msg.messageType}}</td>
        <td>{{moment-format msg.timestamp "lll"}}</td>
        <td>{{moment-format msg.killedAt "lll"}}</td>
        <td>
          <a href="#" {{action onDelete msg}} title="Delete">{{fa-icon 'trash'}}</a>
          <a href="#" {{action onReenqueue msg}} title="Reenqueue">{{fa-icon 'refresh'}}</a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
<button class="btn btn-danger" {{action onDelete 'all'}}>Delete all</button>
<button class="btn btn-warn" {{action onReenqueue 'all'}}>Reenqueue all</button>
