<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>Message Id</th>
      <th>Type</th>
      <th>Timestamp</th>
      <th>Killed at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    {{#each messages as |msg|}}
      <tr>
        <td>{{msg.messageId}}</td>
        <td>{{msg.messageType}}</td>
        <td>{{moment-format msg.timestamp "lll"}}</td>
        <td>{{moment-format msg.killedAt "lll"}}</td>
        <td>
          <a href="#" {{action 'delete'}}>
            {{fa-icon 'trash'}}
          </a>
        </td>
      </tr>
    {{/each}}
  </tbody>
</table>
