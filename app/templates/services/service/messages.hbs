{{#if (eq messageType 'recurring')}}
  {{recurring-messages messages=model
    onDelete=(action 'deleteRecurringMessage')
    onScheduleImmediately=(action 'scheduleImmediateRecurringMessage')
  }}
{{/if}}
{{#if (eq messageType 'dead')}}
  {{dead-messages messages=model
    onDelete=(action 'deleteDeadMessage')
    onReenqueue=(action 'reenqueueDeadMessage')
  }}
{{/if}}
