{{#if (eq messageType 'recurring')}}
  {{recurring-messages messages=model meta=meta
    onDelete=(action 'deleteRecurringMessage')
    onScheduleImmediately=(action 'scheduleImmediateRecurringMessage')
  }}
{{/if}}
{{#if (eq messageType 'dead')}}
  {{dead-messages messages=model meta=meta
    onDelete=(action 'deleteDeadMessage')
    onReenqueue=(action 'reenqueueDeadMessage')
  }}
{{/if}}
{{#if (eq messageType 'schedule')}}
  {{scheduled-messages messages=model meta=meta
    onDelete=(action 'deleteScheduledMessage')
    onEnqueue=(action 'enqueueScheduledMessage')
  }}
{{/if}}
